COMPILER Coconut

CHARACTERS
    digit = '0'..'9'.

TOKENS
    number = digit{digit}.

IGNORE "\n"
PRODUCTIONS
   Coconut
   = { "Coconut"
        basicBlocks
     }.

   basicBlocks
   = basicBlock
     {
       basicBlocks
     }.

   basicBlock
   = {
       instr
     }
     term.

   instr
   = term.

   term <out int n>    (. n=0; .)
   = ['+'] number      (. System.out.println("Positive Number: " + t.val); .)
    | '-' number       (. System.out.println("Negative Number: " + t.val); .)
    .

END Coconut.
